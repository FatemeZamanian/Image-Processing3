    "import cv2\n",
    "rootdir = 'C:/Users/fateme/Desktop/PyClass/PyCourse-23/Contrast'\n",
    "org=cv2.imread(rootdir+'/low.png')\n",
    "org=cv2.cvtColor(org,cv2.COLOR_RGB2GRAY)\n",
    "a=2\n",
    "b=220\n",
    "new = a * org + b\n",
    "cv2.imshow('',new)\n",
    "cv2.waitKey()"
